{
  "name": "@naerth/commander-autocomplete",
  "version": "0.0.3",
  "module": "./src/index.ts",
  "main": "./src/index.ts",
  "type": "module",
  "description": "Small utility that add autocomplete for commander",
  "keywords": [
    "commander",
    "autocompletion",
    "autocomplete"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Naerth/commander-autocomplete"
  },
  "author": {
    "name": "Naerth",
    "url": "https://github.com/Naerth"
  },
  "license": "MIT",
  "exports": {
    "types": "./src/index.ts",
    "import": "./src/index.ts",
    "require": "./src/index.ts"
  },
  "files": [
    "./src/**/*",
    "./tsconfig.json"
  ],
  "scripts": {
    "yarn": "bun install --yarn",
    "build": "bun run build:esm && bun run build:cjs",
    "build:esm": "bun run build:node:esm && bun run build:bun:esm",
    "build:cjs": "bun run build:node:cjs && bun run build:bun:cjs",
    "build:node:esm": "bun build --format esm --target=node ./src/index.ts --outfile=./dist/esm/node/autocomplete.js",
    "build:bun:esm": "bun build --format esm --target=bun ./src/index.ts --outfile=./dist/esm/bun/autocomplete.js",
    "build:node:cjs": "exit;bun build --format cjs --target=node ./src/index.ts --outfile=./dist/cjs/node/autocomplete.js",
    "build:bun:cjs": "exit;bun build --format cjs --target=bun ./src/index.ts --outfile=./dist/cjs/bun/autocomplete.js",
    "postbuild": "tsc -p ./tsconfig.build.json",
    "publish": "npm publish --access public"
  },
  "devDependencies": {
    "bun-types": "latest",
    "commander": "^11.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
