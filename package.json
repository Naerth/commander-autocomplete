{
  "name": "@naerth/commander-autocomplete",
  "version": "0.0.4",
  "module": "./dist/esm/index.js",
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "type": "module",
  "description": "Small utility that add autocomplete for commander",
  "keywords": [
    "commander",
    "autocompletion",
    "autocomplete"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Naerth/commander-autocomplete"
  },
  "author": {
    "name": "Naerth",
    "url": "https://github.com/Naerth"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "files": [
    "./dist/**/*",
    "./tsconfig.json"
  ],
  "scripts": {
    "yarn": "bun install --yarn",
    "clear:dist": "rm -rf ./dist",
    "build": "bun run clear:dist && bun run build:esm && bun run build:cjs",
    "build:cjs": "tsc -p ./tsconfig.build.cjs.json",
    "postbuild:cjs": "bun ./scripts/add-cjs-package-json.ts",
    "build:esm": "tsc -p ./tsconfig.build.esm.json",
    "compile": "tsc -b ./tsconfig.build.cjs.json ./tsconfig.build.esm.json ./tsconfig.build.types.json",
    "publish": "npm publish --access public"
  },
  "devDependencies": {
    "bun-types": "latest",
    "commander": "^11.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
