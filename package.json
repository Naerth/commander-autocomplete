{
  "name": "@naerth/commander-autocomplete",
  "version": "0.0.9",
  "module": "./dist/esm/index.js",
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "type": "module",
  "description": "Small utility that add autocomplete for commander",
  "keywords": [
    "commander",
    "autocompletion",
    "autocomplete"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Naerth/commander-autocomplete"
  },
  "author": {
    "name": "Naerth",
    "url": "https://github.com/Naerth"
  },
  "license": "MIT",
  "exports": {
    "./dev.js": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "default": "./src/index.ts"
    },
    ".": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/node/commander-autocomplete.js",
      "import": "./dist/node/commander-autocomplete.js",
      "bun": "./dist/bun/commander-autocomplete.js",
      "node": "./dist/node/commander-autocomplete.js"
    }
  },
  "files": [
    "./dist/**/*",
    "./tsconfig.json"
  ],
  "scripts": {
    "yarn": "bun install --yarn",
    "clear:dist": "rm -rf ./dist",
    "build": "bun ./scripts/build.ts",
    "postbuild": "tsc -p ./tsconfig.build.types.json",
    "prepublish": "bun run test",
    "publish": "npm publish --access public",
    "test": "bun test --coverage --preload ./test/mock/fs.ts"
  },
  "devDependencies": {
    "commander": "^11.0.0",
    "jest": "^29.7.0",
    "ts-node": "^10.9.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@types/bun": "^1.1.13",
    "@types/jest": "^29.5.14",
    "ts-jest": "^29.2.5"
  }
}